<table class="table">
    <thead>
        <tr>        
           <th scope="col">email</th>
           <!-- <th scope="col">pass</th>-->
            <th scope="col">nombre</th>
            <th scope="col">apellido</th>
        </tr>
    </thead>

    <tbody>
        <% usuarios.forEach(u=> { %>
    <tr>
          
        <td>
            <%= u.email %>
           <!-- <%= u.pass%>-->
           <!-- <%= u.nombre%>-->
            <%= u.apellido%>
        </td>
        <td><a href="/usuarios/editar/<%= u.id %>">✏</a><a href="/usuarios/borrar/<%= u.id %>">🗑️</a>
            <% if (u.deleted_at) { %>
                <a href="/usuarios/restore/<%= u.id %>">♻</a>
                <% } %>
        </td>
    </tr>
        <% }) %>
    </tbody>
</table>