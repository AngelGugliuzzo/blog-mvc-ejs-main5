<table class="table">
    <thead>
        <tr>        
           <th scope="col">email</th>
           <th scope="col">pass%</th>
            <th scope="col">nombre</th>
            <th scope="col">apellido</th>
        </tr>
    </thead>
    <tbody>
        <% usuario.forEach(u=> { %>
    <tr>
          
        <td>
            <%= u.email %>
            <%= u.pass%>
            <%= u.nombre%>
            <%= u.apellido%>
        </td>
       
    </tr>
        <% }) %>
    </tbody>
</table>